{
  "name": "maybe-monada",
  "version": "1.0.4",
  "description": "A maybe monad in JavaScript",
  "main": "dist/maybe.node.js",
  "browser": "dist/maybe.js",
  "scripts": {
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "transpile": "webpack --config buildTools/webpack.config.js",
    "test": "ava tests/",
    "preversion": "npm test",
    "version": "npm run lint && npm run transpile && git add -A dist",
    "postversion": "git push origin dev && git push --tags"
  },
  "keywords": [
    "maybe",
    "monad",
    "javascript",
    "maybe-monad"
  ],
  "author": "Pablo Fernandez",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.3",
    "eslint": "^4.19.1",
    "standard": "^11.0.1",
    "webpack": "^4.10.0",
    "webpack-cli": "^2.1.4",
    "webpack-merge": "^4.1.4"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  }
}
